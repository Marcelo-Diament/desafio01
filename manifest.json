{
  "vendor": "jussiagencia",
  "name": "sandbox",
  "version": "0.0.2",
  "title": "Biblioteca de componentes",
  "description": "Biblioteca de componentes jussilabas para VTEX IO Store Framework",
  "defaultLocale": "pt-BR",
  "builders": {
    "messages": "1.x",
    "store": "0.x",
    "react": "3.x",
    "graphql": "1.x",
    "node": "4.x"
  },
  "dependencies": {
    "vtex.styleguide": "9.x",
    "vtex.css-handles": "0.x",
    "vtex.store-graphql": "2.x"
  },
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.giphy.com",
        "path": "*"
      }
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
